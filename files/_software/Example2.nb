            RowBox[{"MaxCellMeasure", "\[Rule]", "Infinity"}]}], "]"}], ",", 
          "2"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"t", "=", 
       RowBox[{"Accumulate", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Area", "[", 
            RowBox[{"Polygon", "@", "#"}], "]"}], "/", 
           RowBox[{"Area", "[", "p", "]"}]}], "&"}], "/@", "s"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Triangulate", " ", "the", " ", 
        RowBox[{"polygon", ".", "Calculate"}], " ", "the", " ", "area", " ", 
        "of", " ", "the", " ", 
        RowBox[{"polygon", ".", "\[IndentingNewLine]", "Associate"}], " ", 
        "each", " ", "triangle", " ", "with", " ", "its", " ", "fraction", 
        " ", "of", " ", "area", " ", "of", " ", "the", " ", 
        RowBox[{"polygon", ".", "\[IndentingNewLine]", "Associate"}], " ", 
        "each", " ", "triangle", " ", "with", " ", "a", " ", "range", " ", 
        "calculated", " ", "as", " ", "the", " ", "sum", " ", "of", " ", 
        "all", " ", "previous", " ", "area", " ", 
        RowBox[{"fractions", ".", "\[IndentingNewLine]", "This"}], " ", 
        "allows", " ", "a", " ", "triangle", " ", "to", " ", "be", " ", 
        "picked", " ", "from", " ", "a", " ", "random", " ", "variable", " ", 
        "generated", " ", "between", " ", "0", " ", "and", " ", "1."}], 
       "*)"}], 
      RowBox[{"Select", "[", 
       RowBox[{
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{"Function", "[", 
           RowBox[{"x", ",", 
            RowBox[{"Flatten", "@", 
             RowBox[{"RandomPointsTriangle", "[", 
              RowBox[{
               RowBox[{"s", "[", 
                RowBox[{"[", 
                 RowBox[{"First", "@", 
                  RowBox[{"FirstPosition", "[", 
                   RowBox[{
                    RowBox[{
                    RowBox[{
                    RowBox[{"x", "<", "#"}], "&"}], "/@", "t"}], ",", 
                    "True"}], "]"}]}], "]"}], "]"}], ",", "1"}], "]"}]}]}], 
