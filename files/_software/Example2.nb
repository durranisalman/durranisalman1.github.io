  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"RandomPointsTriangle", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a_", ",", "b_", ",", "c_"}], "}"}], ",", "n_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"u", ",", "v"}], "}"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"u", ",", "v"}], "}"}], "=", 
       RowBox[{"Transpose", "[", 
        RowBox[{"Sort", "/@", 
         RowBox[{"RandomReal", "[", 
          RowBox[{"1", ",", 
           RowBox[{"{", 
            RowBox[{"n", ",", "2"}], "}"}]}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"#", " ", "a"}], "&"}], ",", "u"}], "]"}], "+", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"#", " ", "b"}], "&"}], ",", 
         RowBox[{"(", 
          RowBox[{"v", "-", "u"}], ")"}]}], "]"}], "+", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"#", " ", "c"}], "&"}], ",", 
