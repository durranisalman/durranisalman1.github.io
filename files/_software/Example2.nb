          RowBox[{"RandomReal", "[", 
           RowBox[{"1", ",", "n"}], "]"}]}], "]"}], ",", 
        RowBox[{
         RowBox[{"#", "\[NotEqual]", 
          RowBox[{"{", "}"}]}], "&"}]}], "]"}]}]}], "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"Random", " ", 
       RowBox[{"points", ":", 
        RowBox[{
        "Generate", " ", "n", " ", "random", " ", "numbers", " ", "between", 
         " ", "0", " ", "and", " ", "1.", " ", "For", " ", "each", " ", 
         "random", " ", "number"}]}]}], ",", 
      RowBox[{
      "pick", " ", "the", " ", "corresponding", " ", "triangle", " ", "and", 
       " ", "generate", " ", "a", " ", "point", " ", "in", " ", 
       RowBox[{"it", "."}]}]}], "*)"}], "\[IndentingNewLine]", 
    "]"}]}]}]], "Input",
